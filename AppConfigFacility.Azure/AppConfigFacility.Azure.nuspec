<?xml version="1.0" encoding="UTF-8" ?>
<package>
    <metadata>
        <id>WindsorAppConfigFacility.Azure</id>
        <version>$version$</version>
        <authors>Adam Connelly</authors>
        <description>Azure extensions to the Windsor AppConfigFacility</description>
        <language>en</language>
        <projectUrl>https://github.com/adamconnelly/WindsorAppConfigFacility</projectUrl>
        <license type="expression">MIT</license>
        <releaseNotes>Update to support Windsor 5. NOTE: older versions are no-longer supported.</releaseNotes>

        <dependencies>
            <dependency id="WindsorAppConfigFacility" version="$version$" />
            <dependency id="Castle.Core" version="[4.4.0, 5.0.0)" />
            <dependency id="Castle.Windsor" version="[5.0.0, 6.0.0)" />
            <dependency id="Microsoft.WindowsAzure.ConfigurationManager" version="3.2.1" />
        </dependencies>
    </metadata>

    <files>
        <file src="bin\Release\AppConfigFacility.Azure.dll" target="lib\net45" />
        <file src="bin\Release\AppConfigFacility.Azure.pdb" target="lib\net45" />
        <file src="bin\Release\AppConfigFacility.Azure.xml" target="lib\net45" />
        <file src="**\*.cs" exclude="**\obj\*.*" target="src"/>
    </files>
</package>
